{
  "name": "@grapegram/ui-kit",
  "version": "0.2.4",
  "description": "Grapegram ecosustem UI Kit for Vue 3",
  "keywords": [
    "vue",
    "vue3",
    "ui-kit",
    "components",
    "tailwind",
    "typescript"
  ],
  "author": "Grapegram Team",
  "license": "MIT",
  "homepage": "https://github.com/Grapegram/ui-kit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Grapegram/ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Grapegram/ui-kit/issues"
  },
  "type": "module",
  "main": "./dist/ui-kit.umd.js",
  "module": "./dist/ui-kit.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ui-kit.es.js",
      "require": "./dist/ui-kit.umd.js"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "default": "./dist/ui-kit.css"
    }
  },
  "scripts": {
    "build": "vue-tsc && vite build",
    "lint": "eslint . --fix",
    "format": "prettier --write --ignore-unknown src/",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy-storybook": "npx chromatic",
    "release:major": "npm version $(semver $npm_package_version -i major) && git push && git push --tags",
    "release:minor": "npm version $(semver $npm_package_version -i minor) && git push && git push --tags",
    "release:patch": "npm version $(semver $npm_package_version -i patch) && git push && git push --tags"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-vue-next": "^0.526.0",
    "reka-ui": "^2.5.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@storybook/addon-a11y": "^9.1.7",
    "@storybook/addon-docs": "^9.1.7",
    "@storybook/addon-onboarding": "^9.1.7",
    "@storybook/addon-vitest": "^9.1.7",
    "@storybook/vue3-vite": "^9.1.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.15.32",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "chromatic": "^13.3.0",
    "eslint": "^9.29.0",
    "eslint-plugin-storybook": "^9.1.7",
    "eslint-plugin-vue": "~10.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.2.3",
    "npm-run-all2": "^8.0.4",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "semver": "^7.7.3",
    "storybook": "^9.1.7",
    "typescript": "~5.8.0",
    "vite": "^7.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-static-copy": "^3.1.3",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vitest": "^3.2.4",
    "vue": "^3.5.17",
    "vue-tsc": "^2.2.10",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.6"
  },
  "peerDependencies": {
    "vue": ">=3",
    "tailwindcss": ">=4",
    "tw-animate-css": ">=1"
  }
}